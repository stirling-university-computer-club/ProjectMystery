// A = area
	// LOOK = area.look command
// I = item

<a id="0">
	<look>You see dank memes</look>
	<east attr="laptop.turnedOn">
		<goto>1</goto>
	</east>
	<i name="laptop">
		<attributes>turnedOff</attributes>
		<a command="turn on">
			<c attr="turnedOff">
				<changeAttribute>turnedOff turnedOn</changeAttribute>
			</c>
	</i>
</a>
<a id="1">
	<look>You see the dankest of maymays</look>
</a>
	